{
  "version": 2,
  "name": "university-evaluation-system",
  "projects": [
    {
      "name": "frontend",
      "root": "packages/frontend",
      "build": {
        "env": {
          "NODE_ENV": "production"
        }
      }
    },
    {
      "name": "backend",
      "root": "packages/backend",
      "build": {
        "env": {
          "NODE_ENV": "production"
        }
      },
      "routes": [
        {
          "src": "/(.*)",
          "dest": "/api/$1"
        }
      ]
    }
  ]
}
